{
    "import": ["tpl_cartAddEvents"],
    "eventSchema": {
        "populatedAndOfType": {
            "order_id": "!!",
            "prod_list": "!!"
        },
        "fullOrRegExMatch": {
            "prod_action": "add_cart",
            "component_category": "ecommerce",
            "component_subcategory": "add_cart",
            "page_type": "Cart"
        },
        "functionMatch": {
            "cart_content": "cartContentNotEmpty"
        }
    }
}
